<itest>
    <toplogy id="puppet-34-unix" enableByDefault="true">
        <placeholders>
            <host>puppet-34-unix</host>
        </placeholders>
        <targets>
            <target id="Infrastructure/puppet-34-unix">
            </target>
        </targets>
        <configurationItems>
            <overthere.SshHost id="Infrastructure/puppet-34-unix">
                <os>UNIX</os>
                <connectionType>SUDO</connectionType>
                <address>${puppet-34-unix}</address>
                <username>vagrant</username>
                <password>vagrant</password>
                <sudoUsername>vagrant</sudoUsername>
                <puppetPath>/usr/local/bin/</puppetPath>
            </overthere.SshHost>
        </configurationItems>
    </toplogy>
</itest>
